{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "build/**", "!.svelte-kit/cache/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"preview": {
			"dependsOn": ["build"],
			"cache": false,
			"persistent": true
		},
		"check": {
			"dependsOn": ["^check"]
		},
		"check:watch": {
			"cache": false,
			"persistent": true
		}
	}
}
